/*导航栏样式*/
.search-container{
  background: #E8161B;
}
.search-container1{
  background: #E8161B;
  width:100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position:fixed;/* 固定头部 */
  top:0px;
  z-index:999;
}
.search-content{
	width: 710rpx;
	height: 60rpx;
	background: #FFFFFF;
	line-height:60rpx;
	border-radius:30rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.search-content-img-area{
	width:32rpx;
	height:32rpx;
	line-height:32rpx;
	margin-left:24rpx;
}
.search-content-img{
	width:32rpx;
	height:32rpx;
}
.search-content-title{
	font-size:28rpx;
	color:#969696;
	margin-left: 24rpx;
}
.nav-container{
  display: flex;
  flex-direction: row;
  align-items:center;/* Y轴居中对齐*/
  background-color:#E8161B;
  position:fixed;/* 固定头部 */
  top:85rpx;
  z-index:999;
  width: 750rpx;
}
/* 滚动可视区域*/
.nav-scoller-container{
  width:672rpx;
}
/*横向导航*/
.swiper-nav{
  background-color: #E8161B;
  white-space: nowrap;
}
.swiper-nav .item-nav{
  position: relative;
  padding: 22rpx 25rpx;
  color: #FFE5E3;
  font-size: 28rpx;
  cursor: pointer;
  display: inline-block;
  -webkit-tap-highlight-color:rgba(0,0,0,0);
}
.swiper-nav .active{
  color: #ffffff;
	font-size: 32rpx;
	font-weight: bold;
}
.swiper-nav .active::after{
  content:'';
}
.nav-scoller-view-more-area{
	background: #E8161B;
	width:78rpx;
}
.nav-scoller-view-more{
  width:48rpx;
  height: 48rpx;
  margin-left:10rpx; 
	background: url('data:image/png;base64,');
	background-size:100%;
	background-repeat:no-repeat;
}
.nav-scoller-view-more-img{
  width:48rpx;
  height: 48rpx;
}
