page{
	background: #F5F5F5;
}

.banner-container{
	width: 750rpx;
	height: 280rpx;
	background: #BA0B0B;
}
.title-container{
	width: 100%;
	height: 98rpx;
	text-align: center;
	line-height: 98rpx;
	font-weight: bold;
}
/*商品列表样式*/
.goods-container{
  display: flex;
  flex-wrap: wrap;
  flex-direction:row; 
  width:100%;
}
.goods-content{
  /* flex: 0 0 46%; */
  margin-left:10px; 
  margin-top: 10px;
  width: 346rpx;
  height: 544rpx;
  background: #ffffff;
}
.goods-content-img{
  width: 346rpx;
  height: 346rpx;
	mode:widthFix;
}
.goods-content-title-area{
  height: 80rpx;
  line-height: 30rpx;
  padding:1rpx 4px;
}
.goods-content-title{
  color: #333333;
  font-size:24rpx; 
}
.goods-content-tg-price-area{
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  padding:0rpx 16rpx;
  margin-top: 8rpx;
}

.goods-content-quan-title{
	margin-right: 12rpx;
}
.goods-content-tg-price{
  color: #FFFFFF;
  font-size:24rpx; 
}
.goods-content-tg2-price{
  color: #969696;
  font-size:24rpx; 
  text-decoration:line-through;
}
.goods-content-tg3-price{
  color: #969696;
  font-size:24rpx; 
}
.goods-content-get-area{
  display:flex;
  flex-direction:row;
  justify-content:space-between;
	padding:0rpx 4rpx;
}
.goods-sale-content-left{
	display: flex;
	flex-direction: row;
	align-items: center;
}
.goods-sale-content1{
	color: #E8161B;
	font-size: 26rpx;
}
.goods-sale-content2{
	color: #E8161B;
	font-size: 36rpx;
	font-weight: bold;
}
.goods-sale-content3{
	color: #969696;
	font-size: 26rpx;
	text-decoration:line-through;
	margin-left: 4rpx;
}
.goods-sale-content-right{
	display: flex;
	flex-direction: row;
	align-items: center;
	font-size: 24rpx;
	color: #969696;
}
.goods-content-quan-area{
  display:flex;
  flex-direction:row;
	margin-top: 6rpx;
	margin-left: 6rpx;
}
