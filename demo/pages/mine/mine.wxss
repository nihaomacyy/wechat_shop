.upgradeClose{
	width: 312rpx;
	height: 80rpx;
	text-align: center;
	line-height: 80rpx;
	color: #483919;
	background: -webkit-linear-gradient(left, #EFD596 , #D7AA50); /* Safari 5.1 - 6.0 */
	background: -o-linear-gradient(right, #EFD596,#D7AA50); /* Opera 11.1 - 12.0 */
	background: -moz-linear-gradient(right, #EFD596, #D7AA50); /* Firefox 3.6 - 15 */
	background: linear-gradient(to right, #EFD596 , #D7AA50); /* 标准的语法 */
	position:relative;
	top:-260rpx;
	left:98rpx;

}