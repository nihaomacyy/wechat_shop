
/* 商品列表样式 */
.goods-container{
  display: flex;
  flex-direction: row;
  width:100%;
  height: 248rpx;
  background: #ffffff;
}
.goods-content-img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.goods-content-img-area{
  width: 200rpx;
  height: 200rpx;
  margin-top: 24rpx;
  margin-left:24rpx; 
}
.goods-content-word-area{
  display: flex;
  flex-direction: column;
  margin-top: 24rpx;
  margin-left: 20rpx;
  margin-right:24rpx;
}
.goods-content-title-area{
  height: 72rpx;
  line-height:36rpx;
  color: #000000;
  font-size:28rpx;
}
.goods-content-get-area{
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items: center;
  line-height:56rpx;
  height:56rpx;
}
.goods-content-get-price{
  color: #E8161B;
  font-size:24rpx; 
	font-weight: bold;
}
.goods-content-get-price1{
  color: #E8161B;
  font-size:32rpx;
	font-weight: bold;
}
.goods-content-quan{
	background:url('data:image/png;base64,');
	background-repeat:no-repeat;
	background-size:100%;
	color: #FFFFFF;
	font-size:24rpx;
	width:88rpx;
	height:36rpx;
	line-height:36rpx;
	text-align:right;
}
.goods-content-quan1{
	background:url('data:image/png;base64,');
	background-repeat:no-repeat;
	background-size:100%;
	color: #FFFFFF;
	font-size:24rpx;
	width:104rpx;
	height:36rpx;
	line-height:36rpx;
	text-align:right;
}
.goods-content-quan2{
	background:url('data:image/png;base64,');
	background-repeat:no-repeat;
	background-size:100%;
	color: #FFFFFF;
	font-size:24rpx;
	width:120rpx;
	height:36rpx;
	line-height:36rpx;
	text-align:right;
}
.goods-content-quan-title{
	margin-right: 12rpx;
}

.goods-content-footer-area{
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: space-between;
}

.goods-content-footer-price{
  font-size: 24rpx;
  color: #808080;
}

.goods-content-footer-tg-price{
  color: #FFFFFF;
  font-size:28rpx;
  background: #F44336;
  padding:4rpx 40rpx;
  border-radius: 24rpx;
  margin-top:20rpx;
}