/* 商品大图样式 */
.bgView{
  width: 100%;
  overflow: hidden;
  background: #F5F5F5;
}
.goods-img-container{
  width: 750rpx;
  height: 750rpx;
  background: #dfdfdf;
}
.goods-img{
  width: 100%;
  height:100%;
}


/** 商品详情样式*/
.goods-detail-container{
  margin-top:24rpx;
	margin-bottom:98rpx;
}
.goods-detail-title-area{
	width:100%;
	height:88rpx;
	background:#ffffff;
	text-align:center;
	line-height:88rpx;
}
.goods-detail-title{
	color:#333333;
	font-size:28rpx;
}
.goods-detail-area{
	background: #FFFFFF;
	text-align: center;
}
.goods-detail-img{
	/* width: 100%; */
}
.goods-quan{
	font-size: 24rpx;
	color: #FFFFFF; 
	width:120rpx;
	height:40rpx;
	line-height:40rpx;
	text-align:center;
	margin-left:20rpx;
	background: url('data:image/png;base64,') no-repeat;background-size:100%;
}

/**
 * 海报样式
 */
.poster-container{
	width:440rpx;
	height:1100rpx;
}
.poster-container-save{
	width: 440rpx;
	height: 88rpx;
	background: #FF6F00;
	line-height: 88rpx;
	margin-top:24rpx;
	text-align: center;
}
.poster-container-share{
	width: 440rpx;
	height: 88rpx;
	background: #F44236;
	line-height: 88rpx;
	margin-top:24rpx;
	text-align: center;
}
.poster-container-save-title{
	color: #FFFFFF;
	font-size: 36rpx;
}
.poster-container-share-title{
	color: #FFFFFF;
	font-size: 40rpx;
}
.poster-container-img{
	width: 440rpx;
	height: 780rpx;
	/* background: #FFFFFF; */
	text-align: center;
}
.poster-canvas{
	position:none;
	height:0px;
	width:0px;
}
.home{
	position:fixed;
	bottom:60px;
	left:12px;
	width:70rpx;
	height:70rpx;
	text-align:center;
	background: url('data:image/png;base64,');
	background-repeat:no-repeat;
	background-size:100%;
	border:1px solid #ff4336;
	border-radius:35rpx;
}
/* 底部浮动样式*/
.footer-container{
  width: 700rpx;
  position:fixed;
  bottom: 1rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-left:24rpx;
  border-radius:8rpx;
}
.footer-content-area1{
  height: 88rpx;
  width: 700rpx;
  text-align:center;
  line-height:88rpx;
  color:#ffffff;
  font-size: 40rpx;
  background:#E8161B;
  border-radius:8rpx;
  font-weight: bold;
}
/** 标题样式 */
.goods-title-container{
	width:100%;
	height:150rpx;
	padding-left:24rpx;
	background: #FFFFFF;
}

.goods-title-content{
	display:flex;
	flex-direction:row;
	align-items:center;
	margin-bottom:25rpx;
}
.goods-title-content-title1{
	font-size:28rpx;
	color:#333333;
}
.goods-title-content-title2{
	color:#E8161B;
	font-size:28rpx;
	margin-left:16rpx;
}
.goods-title-content-title3{
	font-size:48rpx;
	font-weight:bold;
	color:#E8161B
}
.goods-title-content-title4{
	font-size:24rpx;
	width:112rpx;
	height:36rpx;
	background:#F8D697;
	border-radius:16rpx;
	text-align:center;
	color:#4E3919;
}
.goods-title-content-title5{
	font-size:30rpx;
	font-weight:bold;
	color:#333333;
	margin-left:16rpx;
}
/** 分享信息标识样式 **/
.goods-info-container{
	margin-top: 26rpx;
	background: #FFFFFF;
	width: 100%;
	height: 220rpx;
	display: flex;
	flex-direction: column;
	padding: 0rpx 24rpx;
}
.goods-info-title{
	color: #333333;
	font-size: 28rpx;
	height: 80rpx;
	width: 700rpx;
	border-bottom: 1px solid rgba(222,222,222,0.5);
	line-height: 80rpx;
}
.goods-info-icon-area{
	display: flex;
	flex-direction: row;
	width: 700rpx;
	margin-top:20rpx;
	align-items: center;
	justify-content: space-between;
}
.goods-info-icon{
	text-align: center;
}
.goods-info-icon-img-area{
	width: 60rpx;
	height: 60rpx;
}
.goods-info-icon-img{
	width: 60rpx;
	height: 60rpx;
	mode:widthFix;
}
.goods-info-icon-img-title{
	font-size: 24rpx;
	color: #333333;
}